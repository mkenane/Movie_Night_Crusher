<h3> Back to: <%=link_to("Teams", teams_path) %>
<%=link_to("Users", users_path) %> </h3>

<%= form_for(@team) do |f| %>
  <label>Team Name:</label><br>
  <%= f.text_field :team_name %><br>

  <label>Users Names:</label><br>

  <%= f.collection_check_boxes :user_ids, @users, :id, :username %><br>

  <%= f.submit %>
<% end %>
